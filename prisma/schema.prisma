generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id              Int      @id @default(autoincrement())
  candidate_name  String
  candidate_email String   @unique
  candidate_phone String   @unique
  created_at      DateTime @default(now())

  @@map("registration")
}
